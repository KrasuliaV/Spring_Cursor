<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="plusService" class="xmlConfig.PlusService"/>

    <bean id="minusService" class="xmlConfig.MinusService"/>

    <bean id="calculator" class="xmlConfig.Calculator">
        <constructor-arg type="xmlConfig.PlusService" ref="plusService"/>
        <constructor-arg type="xmlConfig.MinusService" ref="minusService"/>
    </bean>

</beans>